    "jiti": "^2.4.2",
    "terser": "^5.39.0",
    "ts-jest": "^29.2.6",
    "typedoc": "^0.28.1",
    "typedoc-plugin-markdown": "^4.5.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vite": "^6.2.2",
    "vite-plugin-dts": "^4.5.3"
  },
  "files": [
