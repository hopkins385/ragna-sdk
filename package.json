    "lint": "eslint ."
  },
  "keywords": [
    "sdk",
    "typescript",
    "api",
    "ragna"
  ],
  "dependencies": {
    "axios": "^1.8.3"
  },
  "devDependencies": {
    "eslint": "^9.22.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.6",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vite-plugin-dts": "^4.5.3"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hopkins385/ragna-sdk.git"
  },
  "homepage": "https://github.com/hopkins385/ragna-sdk#readme",
